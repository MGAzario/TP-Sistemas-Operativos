[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
[profiles]
  [[default]]
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
  [[kernel]]
    [[[child0]]]
      type = Window
      parent = ""
      order = 0
      position = 0:0
      maximised = True
      fullscreen = False
      title = utnso@utnso: ~
    [[[child1]]]
      type = HPaned
      parent = child0
      order = 0
      ratio = 0.35
    [[[consola]]]
      type = Terminal
      parent = child1
      order = 0
      profile = default
      title = consola kernel
      command = make start
    [[[kernel]]]
      type = Terminal
      parent = child1
      order = 1
      profile = default
      title = logs kernel
      command = tail -f kernel.log
[plugins]